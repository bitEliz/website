{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --cache-location \"node_modules/.cache/stylelint/\" --custom-syntax postcss-html --fix \"**/*.{vue,less,postcss,css,scss}\"",
    "lint:lint-staged": "lint-staged"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --concurrent false",
    "commit-msg": "commitlint -e -V"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.2",
    "highlight.js": "^11.2.0",
    "marked": "^3.0.8",
    "vue": "^3.2.21",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/marked": "^3.0.2",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.2.21",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "postcss-html": "^1.2.0",
    "prettier": "^2.3.2",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-compression": "^0.3.3",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.28.10",
    "yorkie": "^2.0.0"
  }
}
